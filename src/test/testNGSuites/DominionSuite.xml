<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="3" verbose="1" name="Dominion_Test"
	annotations="JDK">
	<listeners>
		<listener class-name="Listeners.TestListener" />
		<listener class-name="Listeners.AnnotationTransformer" />
	</listeners>

	<test name="Chrome" junit="false" preserve-order="true">
		<parameter name="Browser" value="Chrome" />
		<!--<parameter name="Browser" value="headless" />-->
		<groups>
			<run>
				<include name="web" />
			</run>
		</groups>
		<classes>
			<class name="testScripts.DominionScript">
				<methods>
					<include name="MCITEST_236" />
					<include name="MCITEST_240" />
					<include name="MCITEST_243" />
					<include name="MCITEST_239" />
					<include name="MCITEST_308"/>
